# baseURI: http://dramaticallydecayed.com/airdefence
# imports: http://dramaticallydecayed.com/core2ed

@prefix : <http://dramaticallydecayed.com/airdefence#> .
@prefix arg: <http://spinrdf.org/arg#> .
@prefix core2ed: <http://dramaticallydecayed.com/core2ed#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sp: <http://spinrdf.org/sp#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix spl: <http://spinrdf.org/spl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://dramaticallydecayed.com/airdefence>
  rdf:type owl:Ontology ;
  owl:imports <http://localhost:3030/core2ed> ;
  owl:imports <http://localhost:3030/spl> ;
  owl:imports <http://localhost:3030/spin> ;
  owl:imports <http://localhost:3030/sp> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
:AircraftGroup
  rdf:type owl:Class ;
  rdfs:label "Группа" ;
  rdfs:subClassOf core2ed:Complex ;
  rdfs:subClassOf core2ed:WorldHLConcept ;
.
:Danger
  rdf:type owl:Class ;
  rdfs:label "Угроза" ;
  rdfs:subClassOf core2ed:WorldHLConcept ;
.
:DisclosureDanger
  rdf:type owl:Class ;
  rdfs:subClassOf :Danger ;
.
:FormatedGroup
  rdf:type owl:Class ;
  rdfs:label "Построенная группа" ;
  rdfs:subClassOf core2ed:WorldHLConcept ;
.
:IdentifiedAircraft
  rdf:type owl:Class ;
  rdfs:label "Распознанный ЛА" ;
  rdfs:subClassOf core2ed:WorldHLConcept ;
.
:ImportantObject
  rdf:type owl:Class ;
  rdfs:label "Важный объект" ;
  rdfs:subClassOf core2ed:Percepted ;
.
:MarchGroup
  rdf:type owl:Class ;
  rdfs:subClassOf :FormatedGroup ;
.
:ReconnaissanceAircraft
  rdf:type owl:Class ;
  rdfs:subClassOf :IdentifiedAircraft ;
.
:StrikeAircraft
  rdf:type owl:Class ;
  rdfs:subClassOf :IdentifiedAircraft ;
.
:StrikeDanger
  rdf:type owl:Class ;
  rdfs:subClassOf :Danger ;
.
:StrikeGroup
  rdf:type owl:Class ;
  rdfs:subClassOf :FormatedGroup ;
.
:TransportAircraft
  rdf:type owl:Class ;
  rdfs:subClassOf :IdentifiedAircraft ;
.
:UnknownAircraft
  rdf:type owl:Class ;
  rdfs:label "Нераспознанный ЛА" ;
  rdfs:subClassOf core2ed:Percepted ;
.
:associatedWithDanger
  rdf:type owl:ObjectProperty ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:range :Danger ;
  rdfs:subPropertyOf core2ed:associative ;
  rdfs:subPropertyOf core2ed:world ;
.
:beDanger4Object
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Danger ;
  rdfs:range :ImportantObject ;
  rdfs:subPropertyOf core2ed:world ;
.
:computedClassGeneration
  rdf:type core2ed:ComputedClassGeneration ;
.
:cuWorldGeneration
  rdf:type core2ed:CUWorldGeneration ;
.
:dfGeneration
  rdf:type core2ed:DFGeneration ;
.
:elaborationGeneration
  rdf:type core2ed:ElaborationGeneration ;
.
:formGroup
  rdf:type owl:ObjectProperty ;
  rdfs:domain :UnknownAircraft ;
  rdfs:label "входит в состав" ;
  rdfs:range :AircraftGroup ;
  rdfs:subPropertyOf core2ed:complex ;
  rdfs:subPropertyOf core2ed:generative ;
  rdfs:subPropertyOf core2ed:world ;
.
:identifiedAs
  rdf:type owl:ObjectProperty ;
  rdfs:domain :UnknownAircraft ;
  rdfs:range :IdentifiedAircraft ;
  rdfs:subPropertyOf core2ed:generative ;
  rdfs:subPropertyOf core2ed:world ;
.
:identifiedAsFormation
  rdf:type owl:ObjectProperty ;
  rdfs:domain :AircraftGroup ;
  rdfs:label "распознанна как" ;
  rdfs:range :FormatedGroup ;
  rdfs:subPropertyOf core2ed:generative ;
  rdfs:subPropertyOf core2ed:world ;
.
:identify
  rdf:type owl:ObjectProperty ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:range :UnknownAircraft ;
  rdfs:subPropertyOf core2ed:world ;
  owl:inverseOf :identifiedAs ;
.
:inRegionOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ImportantObject ;
  rdfs:range :IdentifiedAircraft ;
  rdfs:subPropertyOf core2ed:associative ;
  rdfs:subPropertyOf core2ed:world ;
.
:levelGeneration
  rdf:type core2ed:LevelGeneration ;
.
#:ndActivation
#  rdf:type core2ed:NDActivation ;
#.
:ndGeneration
  rdf:type core2ed:NDGeneration ;
.
:produceDanger
  rdf:type owl:ObjectProperty ;
  rdfs:domain :FormatedGroup ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:label "создает угрозу" ;
  rdfs:range :Danger ;
  rdfs:subPropertyOf core2ed:world ;
.
:producedByAircraft
  rdf:type owl:ObjectProperty ;
  rdfs:domain :Danger ;
  rdfs:range :IdentifiedAircraft ;
  rdfs:subPropertyOf core2ed:world ;
  owl:inverseOf :produceDanger ;
.
:threaten
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ImportantObject ;
  rdfs:range :Danger ;
  rdfs:subPropertyOf core2ed:generative ;
  rdfs:subPropertyOf core2ed:world ;
  owl:propertyChainAxiom (
      :inRegionOf
      :produceDanger
    ) ;
.
:threatenedBy
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ImportantObject ;
  rdfs:label "под угрозой" ;
  rdfs:range :Danger ;
  rdfs:subPropertyOf core2ed:generative ;
  rdfs:subPropertyOf core2ed:world ;
  owl:propertyChainAxiom (
      :underFormedGroupDirection
      :produceDanger
    ) ;
.
:underFormedGroupDirection
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ImportantObject ;
  rdfs:label "на пути построенной группы" ;
  rdfs:range :FormatedGroup ;
  rdfs:subPropertyOf core2ed:associative ;
  rdfs:subPropertyOf core2ed:world ;
  owl:propertyChainAxiom (
      :underGroupDirection
      :identifiedAsFormation
    ) ;
.
:underGroupDirection
  rdf:type owl:ObjectProperty ;
  rdfs:domain :ImportantObject ;
  rdfs:label "на пути группы" ;
  rdfs:range :AircraftGroup ;
  rdfs:subPropertyOf core2ed:associative ;
  rdfs:subPropertyOf core2ed:world ;
.

:rcs
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:domain :UnknownAircraft ;
  rdfs:range xsd:integer ;
.

:x
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:domain :ImportantObject ;
  rdfs:domain :UnknownAircraft ;
  rdfs:range xsd:integer ;
.

:y
  rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:integer ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:domain :ImportantObject ;
  rdfs:domain :UnknownAircraft ;
.

:vx
  rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:integer ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:domain :UnknownAircraft ;
.

:vy
  rdf:type owl:DatatypeProperty ;
  rdfs:range xsd:integer ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:domain :UnknownAircraft ;
.

:id
  rdf:type owl:DatatypeProperty ;
  rdfs:domain :IdentifiedAircraft ;
  rdfs:domain :ImportantObject ;
  rdfs:domain :UnknownAircraft ;
  rdfs:range xsd:integer ;
.
