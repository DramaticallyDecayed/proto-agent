                              sp:arg1  owl:Thing ;
                              sp:arg2  rdfs:Literal
                            ] ;
        spl:testResult      false .

spl:Test-instanceOf-typedString
        a                   spl:TestCase ;
        spl:testExpression  [ a        spl:instanceOf ;
                              sp:arg1  "test" ;
                              sp:arg2  xsd:string
                            ] ;
